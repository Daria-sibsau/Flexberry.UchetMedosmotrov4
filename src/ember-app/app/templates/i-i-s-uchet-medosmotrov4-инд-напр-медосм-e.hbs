<h3 class="ui header">{{t "forms.i-i-s-uchet-medosmotrov4-инд-напр-медосм-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.номерДок
      class=(if (v-get validationModel "номерДок" "isInvalid") "error")
      label=(t "forms.i-i-s-uchet-medosmotrov4-инд-напр-медосм-e.номерДок-caption")
      data-test-i-i-s-uchet-medosmotrov4-инд-напр-медосм-e-номерДок=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "номерДок" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-uchet-medosmotrov4-инд-напр-медосм-e.дата-caption"}}</label>
    {{flexberry-simpledatetime
      type="date"
      readonly=readonly
      value=model.дата
      class=(if (v-get validationModel "дата" "isInvalid") "error")
      data-test-i-i-s-uchet-medosmotrov4-инд-напр-медосм-e-дата=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "дата" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-uchet-medosmotrov4-инд-напр-медосм-e.организация-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.организация
      displayAttributeName="наименование"
      autocomplete=true
      relationName="организация"
      projection="ОрганизацияL"
      title=(t "forms.i-i-s-uchet-medosmotrov4-инд-напр-медосм-e.организация-caption")
      readonly=readonly
      componentName="организацияLookup"
      class=(if (v-get validationModel "организация" "isInvalid") "error")
      data-test-i-i-s-uchet-medosmotrov4-инд-напр-медосм-e-организация=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "организация" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.организация.наименование
      class=(if (v-get validationModel.организация "наименование" "isInvalid") "error")
      label=(t "forms.i-i-s-uchet-medosmotrov4-инд-напр-медосм-e.организация.наименование-caption")
      data-test-i-i-s-uchet-medosmotrov4-инд-напр-медосм-e-организация-наименование=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.организация "наименование" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-uchet-medosmotrov4-инд-напр-медосм-e.медУчреждение-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.медУчреждение
      displayAttributeName="наименование"
      autocomplete=true
      relationName="медУчреждение"
      projection="МедУчреждениеL"
      title=(t "forms.i-i-s-uchet-medosmotrov4-инд-напр-медосм-e.медУчреждение-caption")
      readonly=readonly
      componentName="медУчреждениеLookup"
      class=(if (v-get validationModel "медУчреждение" "isInvalid") "error")
      data-test-i-i-s-uchet-medosmotrov4-инд-напр-медосм-e-медУчреждение=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "медУчреждение" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.медУчреждение.наименование
      class=(if (v-get validationModel.медУчреждение "наименование" "isInvalid") "error")
      label=(t "forms.i-i-s-uchet-medosmotrov4-инд-напр-медосм-e.медУчреждение.наименование-caption")
      data-test-i-i-s-uchet-medosmotrov4-инд-напр-медосм-e-медУчреждение-наименование=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.медУчреждение "наименование" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-uchet-medosmotrov4-инд-напр-медосм-e.сотрудники-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.сотрудники
      displayAttributeName="фИО"
      autocomplete=true
      relationName="сотрудники"
      projection="СотрудникиL"
      title=(t "forms.i-i-s-uchet-medosmotrov4-инд-напр-медосм-e.сотрудники-caption")
      readonly=readonly
      componentName="сотрудникиLookup"
      class=(if (v-get validationModel "сотрудники" "isInvalid") "error")
      data-test-i-i-s-uchet-medosmotrov4-инд-напр-медосм-e-сотрудники=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "сотрудники" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.сотрудники.iDСотрудника
      class=(if (v-get validationModel.сотрудники "iDСотрудника" "isInvalid") "error")
      label=(t "forms.i-i-s-uchet-medosmotrov4-инд-напр-медосм-e.сотрудники.iDСотрудника-caption")
      data-test-i-i-s-uchet-medosmotrov4-инд-напр-медосм-e-сотрудники-iDСотрудника=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.сотрудники "iDСотрудника" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.сотрудники.фИО
      class=(if (v-get validationModel.сотрудники "фИО" "isInvalid") "error")
      label=(t "forms.i-i-s-uchet-medosmotrov4-инд-напр-медосм-e.сотрудники.фИО-caption")
      data-test-i-i-s-uchet-medosmotrov4-инд-напр-медосм-e-сотрудники-фИО=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.сотрудники "фИО" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-uchet-medosmotrov4-инд-напр-медосм-e.сотрудники.датаРождения-caption"}}</label>
    {{flexberry-simpledatetime
      type="date"
      readonly=true
      value=model.сотрудники.датаРождения
      class=(if (v-get validationModel.сотрудники "датаРождения" "isInvalid") "error")
      data-test-i-i-s-uchet-medosmotrov4-инд-напр-медосм-e-сотрудники-датаРождения=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.сотрудники "датаРождения" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.сотрудники.должности.наименование
      class=(if (v-get validationModel.сотрудники.должности "наименование" "isInvalid") "error")
      label=(t "forms.i-i-s-uchet-medosmotrov4-инд-напр-медосм-e.сотрудники.должности.наименование-caption")
      data-test-i-i-s-uchet-medosmotrov4-инд-напр-медосм-e-сотрудники-должности-наименование=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.сотрудники.должности "наименование" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.сотрудники.должности.подразделения.наименование
      class=(if (v-get validationModel.сотрудники.должности.подразделения "наименование" "isInvalid") "error")
      label=(t "forms.i-i-s-uchet-medosmotrov4-инд-напр-медосм-e.сотрудники.должности.подразделения.наименование-caption")
      data-test-i-i-s-uchet-medosmotrov4-инд-напр-медосм-e-сотрудники-должности-подразделения-наименование=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.сотрудники.должности.подразделения "наименование" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-uchet-medosmotrov4-инд-напр-медосм-e.вреднФактПроиз-caption"}}</label>
    {{flexberry-groupedit
      componentName="вреднФактПроизGroupEdit"
      mainModelProjection=modelProjection
      modelProjection=modelProjection.attributes.вреднФактПроиз
      content=model.вреднФактПроиз
      readonly=readonly
      orderable=false
      class=(if (v-get validationModel "вреднФактПроиз" "isInvalid") "error")
      data-test-i-i-s-uchet-medosmotrov4-инд-напр-медосм-e-вреднФактПроиз=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "вреднФактПроиз" "message")}}
  </div>
</form>
